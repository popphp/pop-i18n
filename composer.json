{
    "name": "popphp/pop-i18n",
    "description": "Pop I18n Component for Pop PHP Framework",
    "keywords": [
        "i18n",
        "l10n",
        "internationalization",
        "localization",
        "language",
        "locale",
        "translation",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\I18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\I18n\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
